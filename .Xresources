!! generic
*foreground:       #dddddd
*foreground_bold:  #a8a19f
!*cursor:           #a8a19f
*background:       #000000
! black
*color0:           #1b1918
*color8:           #766e6b
*color16:          #000000
! red
*color1:           #f22c40
*color9:           #f22c40
! green
*color2:           #5ab738
*color10:          #5ab738
! yellow
*color3:           #d5911a
*color11:          #d5911a
! blue
*color4:           #407ee7
*color12:          #407ee7
! magenta
*color5:           #6666ea
*color13:          #6666ea
! cyan
*color6:           #00ad9c
*color14:          #00ad9c
! white
*color7:           #f1efee
*color15:          #f1efee

!! sxiv
Sxiv.foreground:   #666666
Sxiv.background:   #000000

!! rofi
rofi.fullscreen:       true
rofi.max-history-size: 50
rofi.color-enabled:    true
!		       'background',  'border',      'dividing line'
rofi.color-window:     argb:EE000000, argb:EE000000, argb:00000000
!		       'bg',          'fg',    'bgalt',       'hlbg',        'hlfg'
rofi.color-normal:     argb:00000000, #e8e8e8, argb:00000000, argb:88111111, #ff008066
rofi.color-urgent:     argb:00000000, #f43753, argb:00000000, argb:88111111, #ff008066
rofi.color-active:     argb:00000000, #49bbfb, argb:00000000, argb:88111111, #ff008066
rofi.font:             Monospace 16
!rofi.fg:               #111111
!rofi.bg:               #000000
rofi.eh:               1
rofi.separator-style:  solid
rofi.hide-scrollbar:   true
rofi.line-margin:      24
rofi.lines:            7
rofi.width:            50
rofi.padding:          200
rofi.bw:               0
rofi.auto-select:      false
rofi.fuzzy:            true
rofi.scroll-method:    1
rofi.window-format:    {t}
